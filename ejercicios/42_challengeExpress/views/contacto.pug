extends layout

block content
  header
    .registro
      a(href='/usuario') Mi cuenta/Registrate
      a(href='/cart')
        i.fas.fa-shopping-cart
    img.logo(src='/images/web/logo.png')
    nav
      a(href='/') Home
      a(href='/productos') Productos
      a(href='/nosotros') Nosotros
      a(href='/como-comprar') C&oacute;mo comprar
      a(href='/contacto') Contacto
  .container
    h1.con Â¿Tenes alguna consulta?
    form.contacto(method='post')
      label(for='nombre') Nombre
      input(type='text' name='nombre' placeholder='Nombre')
      label(for='email') E-mail
      input(type='text' name='email' placeholder='E-mail')
      label(for='consulta') Consulta
      textarea(name='consulta' placeholder='Consulta')
      button.buttonForm#enviarConsulta(type='submit') Enviar
  footer
    section.social-media
      a(href='twitter.com/')
        i.fab.fa-twitter
      a(href='instagram.com/')
        i.fab.fa-instagram
      a(href='facebook.com/')
        i.fab.fa-facebook