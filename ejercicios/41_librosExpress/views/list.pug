extends layout

block content
  header 
    nav.links
      span &gt;
      a(href='/') Home
    h1 #{title}
  div#container
    for libro in libros
      .producto
        figure
          img(src='../'+libro.img)
        a(href=libro.url+libro.cod) #{libro.nombre}
        p #{libro.precio}
        button.submit(type='submit') Comprar